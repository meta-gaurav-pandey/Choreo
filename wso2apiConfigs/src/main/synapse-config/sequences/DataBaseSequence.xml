<?xml version="1.0" encoding="UTF-8"?>
<sequence name="DataBaseSequence" onError="ErrorSequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log separator=",">
        <property name="StartingLog" value="Database sequence started "/>
    </log>
    <property name="sObjectType" scope="default" type="STRING" value="DataEvent"/>
    <sequence key="ClassSequence"/>
    <payloadFactory media-type="json">
        <format>$1</format>
        <args>
            <arg evaluator="json" expression="$ctx:Array"/>
        </args>
    </payloadFactory>
    <log separator=",">
        <property expression="$body" name="Body"/>
        <property expression="$ctx:Array" name="Array"/>
    </log>
    <call>
        <endpoint>
            <http method="post" statistics="enable" uri-template="http://52.91.255.31:8080/ttl/generate/ttl?entityType=OrganisationMapping">
                <suspendOnFailure>
                    <initialDuration>-1</initialDuration>
                    <progressionFactor>-1</progressionFactor>
                    <maximumDuration>0</maximumDuration>
                </suspendOnFailure>
                <markForSuspension>
                    <retriesBeforeSuspension>0</retriesBeforeSuspension>
                </markForSuspension>
            </http>
        </endpoint>
    </call>
    <log level="full"/>
    <call>
        <endpoint>
            <http method="post" uri-template="http://52.91.255.31:8080/insertion/insertDataIntoGraph">
                <suspendOnFailure>
                    <initialDuration>-1</initialDuration>
                    <progressionFactor>-1</progressionFactor>
                    <maximumDuration>0</maximumDuration>
                </suspendOnFailure>
                <markForSuspension>
                    <retriesBeforeSuspension>0</retriesBeforeSuspension>
                </markForSuspension>
            </http>
        </endpoint>
    </call>
    <log separator=",">
        <property name="Ending Sequenec" value="Ending Of the database sequence "/>
    </log>
</sequence>
