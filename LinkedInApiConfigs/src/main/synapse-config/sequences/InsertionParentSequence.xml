<?xml version="1.0" encoding="UTF-8"?>
<sequence name="InsertionParentSequence" onError="ErrorSequence" trace="disable" xmlns="http://ws.apache.org/ns/synapse">
    <log separator=",">
        <property name="StartingLog" value="InsertionSequence Started"/>
    </log>
    <payloadFactory media-type="json">
        <format>$1</format>
        <args>
            <arg evaluator="json" expression="$ctx:Array2"/>
        </args>
    </payloadFactory>
    <call>
        <endpoint>
            <http method="post" statistics="enable" uri-template="http://52.91.255.31:8080/ttl/generate/ttl?entityType=OrganisationMapping">
                <suspendOnFailure>
                    <initialDuration>-1</initialDuration>
                    <progressionFactor>-1</progressionFactor>
                    <maximumDuration>0</maximumDuration>
                </suspendOnFailure>
                <markForSuspension>
                    <retriesBeforeSuspension>0</retriesBeforeSuspension>
                </markForSuspension>
            </http>
        </endpoint>
    </call>
    <log>
        <property name="Insertion parent sequence" value="ttl generation true"/>
    </log>
    <call>
        <endpoint>
            <http method="post" uri-template="http://52.91.255.31:8080/insertion/insertDataIntoGraph">
                <suspendOnFailure>
                    <initialDuration>-1</initialDuration>
                    <progressionFactor>-1</progressionFactor>
                    <maximumDuration>0</maximumDuration>
                </suspendOnFailure>
                <markForSuspension>
                    <retriesBeforeSuspension>0</retriesBeforeSuspension>
                </markForSuspension>
            </http>
        </endpoint>
    </call>
    <log separator=",">
        <property name="EndingLog" value="InsertionSequence End"/>
    </log>
</sequence>
